grammar := definition, (newline, definition)*
definition := name, space?, assignment, space?, syntax
assignment := ':='
space     := [ \t]+
newline     := [\r\n]+
name := identifier
identifier := alphaunder, (PERIOD?, alphanums+)*
PERIOD := '.'
alphanums      := [a-zA-Z0-9]+
alphaunder     := [a-zA-Z_]
syntax := not_newline
not_newline := [^\r\n]+
