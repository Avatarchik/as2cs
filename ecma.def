importDefinition := whitespace?, import, whitespace, identifier, SEMI, EOL

identifier := alphaunder, (PERIOD?, alphanums+)*
alphanums      := (letter / digit)+
alphaunder     := (letter / '_')
dataType := int / string / boolean / float / object

PERIOD := '.'
LCURLY := '{'
RCURLY := '}'
SEMI := ';'
COLON := ':'
EOL   := ('\r'?,'\n') / EOF
