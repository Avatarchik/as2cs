compilationUnit := namespace, (whitespace, identifier)?, whitespace?, LCURLY, EOL, importDefinitions?, RCURLY

compilationUnit := namespaceDecl, namespaceBlock

namespaceDecl := namespace, (whitespacechar+, identifier)?

namespaceBlock := whitespace?, LCURLY, namespaceBlockEntry, RCURLY, whitespace?

namespaceBlockEntry := whitespace?, importDefinition*, whitespace?

variableDeclaration := variableDeclarationKeyword, whitespacechar+, identifier, (COLON, dataType)?, whitespace*, SEMI

int := 'int'
string := 'String'
boolean := 'Boolean'
float := 'number'
object := 'Object'
variableDeclarationKeyword := 'var'

import := 'import'

namespace := 'package'

