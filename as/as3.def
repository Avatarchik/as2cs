compilationUnit := packageDecl, packageBlock

packageDecl := PACKAGE, (whitespacechar+, identifier)?

packageBlock := whitespace?, LCURLY, packageBlockEntry, RCURLY, whitespace?

packageBlockEntry := whitespace?, importDefinition*, whitespace?

variableDeclaration := variableDeclarationKeyword, whitespacechar+, identifier, (COLON, dataType)?, whitespace*, SEMI

int := 'int'
string := 'String'
boolean := 'Boolean'
float := 'number'
object := 'Object'
variableDeclarationKeyword := 'var'

import := 'import'
PACKAGE := 'package'
